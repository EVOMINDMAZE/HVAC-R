  "scripts": {
    "dev": "vite",
    "lint": "eslint .",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "vite build --config vite.config.server.ts",
    "start": "node dist/server/node-build.mjs",
    "test": "vitest --run",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:ux:landing": "node output/playwright/ux/ux_run.mjs",
    "test:ux:landing:headed": "HEADED=1 node output/playwright/ux/ux_run.mjs",
    "test:ux:tracking": "node output/playwright/ux/tracking_verify.mjs",
    "test:ux:tracking:headed": "HEADED=1 node output/playwright/ux/tracking_verify.mjs",
    "test:ux:viewport": "node output/playwright/ux/hero_viewport_check.mjs",
    "test:ux:viewport:headed": "HEADED=1 node output/playwright/ux/hero_viewport_check.mjs",
    "test:ux:a11y": "node output/playwright/ux/accessibility_smoke.mjs",
    "test:ux:a11y:headed": "HEADED=1 node output/playwright/ux/accessibility_smoke.mjs",
    "test:ux:density:hero": "node output/playwright/ux/hero_density_audit_v2.mjs",
    "test:ux:density:hero:headed": "HEADED=1 node output/playwright/ux/hero_density_audit_v2.mjs",
