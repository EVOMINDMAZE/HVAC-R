    "test:ux:future-monitors:headed": "HEADED=1 node output/playwright/ux/future_monitor_route_audit_v1.mjs",
    "test:ux:future-monitors:hud": "node output/playwright/ux/future_monitor_route_audit_v1.mjs --skin hud",
    "test:ux:future-monitors:hud:headed": "HEADED=1 node output/playwright/ux/future_monitor_route_audit_v1.mjs --skin hud",
    "test:ux:suite": "node output/playwright/ux/ux_run.mjs && node output/playwright/ux/tracking_verify.mjs && node output/playwright/ux/hero_viewport_check.mjs && node output/playwright/ux/accessibility_smoke.mjs && node output/playwright/ux/hero_density_audit_v2.mjs && node output/playwright/ux/page_density_audit_v2.mjs",
    "test:ux:suite:headed": "HEADED=1 node output/playwright/ux/ux_run.mjs && HEADED=1 node output/playwright/ux/tracking_verify.mjs && HEADED=1 node output/playwright/ux/hero_viewport_check.mjs && HEADED=1 node output/playwright/ux/accessibility_smoke.mjs && HEADED=1 node output/playwright/ux/hero_density_audit_v2.mjs && HEADED=1 node output/playwright/ux/page_density_audit_v2.mjs",
    "test:setup-auth": "tsx scripts/setup-playwright-auth.ts",
    "format.fix": "prettier --write .",
    "typecheck": "tsc"
  },
