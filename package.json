    "test:ux:signedin:viewport:headed": "HEADED=1 node output/playwright/ux/signed_in_viewport_check.mjs",
    "test:ux:signedin:a11y": "node output/playwright/ux/signed_in_accessibility_smoke.mjs",
    "test:ux:signedin:a11y:headed": "HEADED=1 node output/playwright/ux/signed_in_accessibility_smoke.mjs",
    "test:ux:signedin:suite": "node output/playwright/ux/signed_in_smoke.mjs && node output/playwright/ux/signed_in_viewport_check.mjs && node output/playwright/ux/signed_in_accessibility_smoke.mjs",
    "test:ux:signedin:suite:headed": "HEADED=1 node output/playwright/ux/signed_in_smoke.mjs && HEADED=1 node output/playwright/ux/signed_in_viewport_check.mjs && HEADED=1 node output/playwright/ux/signed_in_accessibility_smoke.mjs",
